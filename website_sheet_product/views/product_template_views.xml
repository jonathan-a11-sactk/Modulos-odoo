<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE xml>
<odoo>
	<data>
	
		<record model="ir.ui.view" id="product_template_only_form_view_website_sheet_product">
			<field name="name">product.template.product.form.inherit.website_sheet_product</field>
			<field name="model">product.template</field>
			<field name="inherit_id" ref="dim_product.product_template_only_form_view_inherit_dim_product"/>
			<field name="arch" type="xml">
			
				<xpath expr="//group[@name='general']" position="after">
					<group string="Website" name="website">
						<field name="website_status"/>
					</group>
				</xpath>

			</field>
		</record>

	</data>
</odoo>